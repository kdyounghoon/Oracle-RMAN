      storage:
        constraints:
          - tag: 'storage:robo'
      vm_prefix: '${input.prefix}'
      count: '${input.vm_count / 50}'
      vm_count: '${input.vm_count}'
      image: '${input.vm_count}'
      customizationSpec: '${input.customizationSpec}'
      cpuCount: '${input.cpu}'
      totalMemoryMB: '${input.memory * 1024}'
